import java.util.Scanner;
class Marksaverage{
   public static void main(String[] args) {
   int mathsmarks=94;
   int physicsmarks=95; 
   int chemistrymarks=96;
   int marksaverage=(mathsmarks+physicsmarks+chemistrymarks)/3;
   System.out.println("Sam's average mark in PCM is "+marksaverage);
   }
}
