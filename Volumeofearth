class Volumeofearth{
   public static void main(String[] args) {
   int radius=6378;
   double volumeofearthkm=(4.0/3.0)*3.14*radius*radius*radius;
   double volumeofearthmiles=volumeofearthkm/1.6;
   System.out.println("The volume of earth in cubic kilometer is "+volumeofearthkm+" and cubic miles is "+volumeofearthmiles);
   }
}
